<% if @privilege %>
new Messi("<%=  MESSAGES["previlages"]["create"]["success"] %>", {title: 'Success', titleClass: 'success'});
<% @created_priviliges.each do |privilege| %>
 dataSource.insert(0,{id: <%= privilege.id%>, role_name: "<%= privilege.role_title%>", model_name: "<%= privilege.modular_model_name%>", action_name: "<%= privilege.modular_action_name%>"});	
<%end%>
$('#privilege_modular').val('');
$('.multiselect').multiselect().trigger('reset');
<% end %>
setTimeout(function(){$('.messi-closebtn').trigger("click")},1000);
