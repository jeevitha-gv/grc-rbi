<div class="page-wrap">
  <div class="content">
    <div class="container">
      <div class="row">
        <div id="example" class="k-content">
          <div class="risk-title">Edit Business Plan</div>

          <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="risk-wrap">
              <div class="sub-wrap">
                <div class="sub-titile">Business Plan</div>
                <div class="sub-wrap-bg">
                  <%= form_for [@bc_analysis, @bc_paln] do |f| %>
                    <%= render 'form', :f => f, :bc_paln => @bc_paln %>
                    <div class="row">
                      <div class="col-lg-12 col-md-6 col-sm-6 col-xs-12">
                        <div class="btn-group gr-btn save-file">
                          <%#= f.submit "Initiate Mobile Asset", :class => "orange_btn" %>
                          <%= f.submit "Update Business Plan", :class => "grey_btn" %>
                          <a class="black_btn" href="javascript:goBack()">Back</a>
                        </div>
                      </div>
                    </div>
                  <% end %>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>