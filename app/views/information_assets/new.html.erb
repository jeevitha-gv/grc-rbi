<div class="page-wrap">
  <div class="content">
    <div class="container">
      <div class="row">
        <div id="example" class="k-content">
          <div class="risk-title">New Information Asset</div>


        <div class="clearfix"> <a class="import-btn" data-toggle="modal" href="#add_url"><i class="download-icons"></i>Import </a>
            <div id="add_url" class="modal fade">
              <div class="modal-dialog modal-dialog_large">
                <div class="modal-content">
                  <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"> &nbsp; </button>
                    <h4 class="modal-title" id="myModalLabel">Import Information Assets from CSV/XLSX</h4>
                       <div class="modal-content-inner">
                      <%= form_tag infoasset_imports_information_assets_path, multipart: true do %>
                        <%= file_field_tag :file %>
                        <br>
                        <div class="model-footer">
                          <%= submit_tag "Import" ,:class => "black_btn" %>
                          <%= submit_tag "Cancel" ,:class => "orange_btn" %>
                        </div>
                      <% end %>
                    </div>
                    <p></p>
                    <p>
                      <div  class="grey_btn pop-up-gray">
                      <%= link_to "Download Sample Risk CSV Template", risk_export_risks_path(format: "csv"), class: "download-file-icon plus-icon-sp" %>
                    </div>
                    </p>
                  </div>
                </div>
              </div>
            </div>
          </div>


          <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="risk-wrap">
              <div class="sub-wrap">
                <div class="sub-titile">Add a new Asset</div>
                <div class="sub-wrap-bg">
                  <%=form_for(@information_asset) do |f| %>
                    <%= render 'form', :f => f, :information_asset => @information_asset %>
                    <div class="row">
                      <div class="col-lg-12 col-md-6 col-sm-6 col-xs-12">
                        <div class="btn-group gr-btn save-file">
                          <%= f.submit "Add Information Asset", :class => "orange_btn"%>
                        </div>
                      </div>
                    </div>
                  <% end %>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>