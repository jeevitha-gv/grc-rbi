    <link rel="stylesheet" type="text/css" href="/assets/outer/jquery.fancybox.css">
    <link rel="stylesheet" type="text/css" href="/assets/outer/asset.css">
    <link href="/assets/outer/style1.css" rel="stylesheet" media="screen">
    <link href="/assets/messi.min.css" rel="stylesheet" media="screen">
    <link href="/assets/outer/mediaqueries.css" rel="stylesheet" media="screen">
    <link rel="stylesheet" type="text/css" href="/assets/outer/editablestyle.css">
