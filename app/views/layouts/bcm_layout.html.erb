    <link rel="stylesheet" type="text/css" href="/assets/outer/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="/assets/outer/style.css">
    <link rel="stylesheet" type="text/css" href="/assets/outer/docs.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/outer/media.css">
    <link rel="stylesheet" type="text/css" href="/assets/outer/kendo.default.min.css">
