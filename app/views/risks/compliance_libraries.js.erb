<% if params[:compliance_id].present? %>
  <% select = select_tag(:compliance_library_id, options_for_select(@compliance_libraries.collect{|x| [x.name, x.id]}), {:include_blank => 'Please Select',:id => "department_id", :onChange => "get_teams(this);"}) -%>
  $('#risk_compliance_library_id').replaceWith('<%= escape_javascript(select) %>');
  $('.ajax-loader').hide();
<% end %>